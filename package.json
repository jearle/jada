{
  "name": "jada",
  "version": "0.0.0",
  "description": "Jada is a simple, lightweight library for storing data. It does not update localStorage, nor does it provide hooks for syncing data to a server. It simply gets and sets data, and provides hooks for listening to when data has been set both before and after.",
  "main": "lib/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_PATH=./ node_modules/.bin/mocha -R spec --recursive test",
    "browserify-test": "NODE_PATH=./ node_modules/.bin/browserify -d -e test/lib/* -o build/test.js",
    "generate-lodash": "cd vendor/lodash && lodash exports=node include=isPlainObject"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jearle/jada.git"
  },
  "keywords": [
    "keywords"
  ],
  "author": "Jesse Earle",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^1.20.1",
    "chai": "^1.9.1",
    "gulp": "^3.8.2",
    "connect-livereload": "^0.4.0",
    "express": "^4.4.4",
    "tiny-lr": "0.0.7",
    "stylify": "^0.1.5",
    "jade": "^1.3.1",
    "jadeify": "^2.3.0",
    "browserify": "^4.1.11",
    "gulp-rename": "^1.2.0",
    "gulp-uglify": "^0.3.1",
    "gulp-browserify": "^0.5.0",
    "fs-extra": "^0.9.1",
    "gulp-connect": "^2.0.5",
    "jshint": "^2.5.1",
    "gulp-jshint": "^1.6.3",
    "gulp-util": "^2.2.18",
    "jshint-stylish": "^0.2.0",
    "lodash": "^2.4.1",
    "gulp-shell": "^0.2.9"
  }
}
